
#include <bits/stdc++.h>

using namespace std;

int main()
{
    int n;
    cin>>n;
    int a[n];
    for(int i=0;i<n;i++)
    {cin>>a[i];}
    
    //Kadane's Algo
     int endsum=0, maxsum=INT_MIN;
   for(int i=0;i<n;i++)
    {
        endsum+=a[i];
        if(a[i]>endsum)
        endsum=a[i];
        if(endsum>maxsum)
        maxsum=endsum;
        
    }
    cout<<maxsum<<endl;
    return 0;
}
